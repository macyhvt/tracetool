{
	"name": "froetek/trackingtool",
	"type": "project",
	"version": "3.0",
	"description": "Production process tracking advanced.",
	"keywords": [],
	"homepage": "https://bitbucket.org/froetek-development/trackingtool",
	"support": {
		"email": "development@froetek.de",
		"source": "https://bitbucket.org/froetek-development/trackingtool",
		"issues": "https://bitbucket.org/froetek-development/trackingtool/issues"
	},
	"license": "proprietary",
	"minimum-stability": "alpha",
	"authors": [
		{
			"name": "Tino Brackebusch",
			"email": "development@froetek.de",
			"role": "Developer"
		}
	],
	"bin": [
		"bin/nematrack"
	],
	"require": {
		"php": ">=7.4",
		"ext-curl": "@stable",
		"ext-fileinfo": "^7.4",
		"ext-gd": "^7.4",
		"ext-imagick": "^3.4",
		"ext-intl": "^7.4",
		"ext-json": "^7.4",
		"ext-xmlreader": "^7.4",
		"ext-zip": "^1.15",
		"box/spout": "^3.2",
		"defuse/php-encryption": "^2.3",
		"hackzilla/password-generator": "^1.4",
		"joomla/database": "^1.7",
		"joomla/filesystem": "^2.0@beta",
		"joomla/image": "^1.5",
		"joomla/language": "^1.4",
		"joomla/model": "^1.3",
		"joomla/registry": "^1.6",
		"joomla/router": "^1.2",
		"joomla/string": "^1.4",
		"joomla/uri": "^1.2",
		"joomla/utilities": "^1.6",
		"kusmierz/xpdfbin-amd64": "^3.3",
		"monolog/monolog": "^2.2",
		"sabre/vobject": "^4.3",
		"shuchkin/simplexlsxgen": "^1.0",
		"symfony/filesystem": "^5.2",
		"symfony/finder": "^5.2",
		"symfony/http-foundation": "^5.2",
		"symfony/mailer": "^5.4",
		"symfony/mime": "^5.3",
		"symfony/security-csrf": "^5.2",
		"symfony/twig-bundle": "^5.4",
		"symfony/validator": "^5.3",
		"wamania/php-stemmer": "^3.0",
		"wenzhixin/bootstrap-table": "1.16.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "@stable",
		"joomla/coding-standards": "~2.0",
		"league/glide": "^2.2",
		"league/glide-symfony": "^2.0",
		"matthiasmullie/minify": "^1.3",
		"maximebf/debugbar": "^1.17",
		"phpmd/phpmd": "@stable",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpdoc-parser": "^1.8",
		"phpstan/phpstan": "^1.8",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"phpstan/phpstan-phpunit": "^1.1",
		"phpstan/phpstan-strict-rules": "^1.4",
		"squizlabs/php_codesniffer": "^2.8",
		"smalot/pdfparser": "@stable",
		"symfony/stopwatch": "@stable",
		"symfony/var-dumper": "@stable",
		"tobyz/json-api-server": "^0.2.0@beta",
		"voku/html-min": "^4.0",
		"voku/portable-utf8": "^5.4",
		"wyrihaximus/html-compress": "^4.2"
	},
	"autoload": {
		"psr-4": {
			"Nematrack\\": "./src"
		},
		"files": [
			"./src/Defines.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Nematrack\\": "./tests"
		}
	},
	"config": {
		"process-timeout": 0,
		"optimize-autoloader": true,
		"sort-packages": true,
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
